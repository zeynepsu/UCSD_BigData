      "%%writefile maxmin.py\n",
      "#!/usr/bin/python\n",
      "\"\"\"\n",
      "count the number of measurements of each type\n",
      "\"\"\"\n",
      "import sys\n",
      "import numpy as np\n",
      "sys.path.append('/usr/lib/python2.6/dist-packages')\n",
      "from mrjob.job import MRJob\n",
      "import re\n",
      "from sys import stderr\n",
      "\n",
      "class MRWeather(MRJob):\n",
      "\n",
      "    def mapper(self, _, line):\n",
      "        try:\n",
      "            self.increment_counter('MrJob Counters','mapper-all',1)\n",
      "            elements=line.split(',')\n",
      "            number_defined=sum([e!='' for e in elements[3:]])\n",
      "            if number_defined==365 and elements[1]=='TMAX':\n",
      "                if elements[0]!='station':\n",
      "                    yield ([elements[0],elements[2]],[1]+elements[:])\n",
      "            if number_defined==365 and elements[1]=='TMIN':\n",
      "                if elements[0]!='station':\n",
      "                    yield ([elements[0],elements[2]],[1]+elements[:])\n",
      "\n",
      "        except Exception, e:\n",
      "            stderr.write('Error in line:\\n'+line)\n",
      "            stderr.write(e)\n",
      "            self.increment_counter('MrJob Counters','mapper-error',1)\n",
      "\n",
      "            \n",
      "\n",
      "    def reducer(self, word, counts):\n",
      "        self.increment_counter('MrJob Counters','reducer',1)\n",
      "        new=[]\n",
      "        for v in counts:\n",
      "            if v[2]=='TMAX':\n",
      "                new=v[4:]+new\n",
      "            else:\n",
      "                new=new+v[4:]\n",
      "        tmp='1'\n",
      "        if len(new)==730:\n",
      "            for s in new:\n",
      "                tmp=tmp+' '+s\n",
      "            tmp=tmp+' 1'\n",
      "            yield (word[0],[tmp])\n",
      "        #l_counts=[c for c in counts]  # extract list from iterator\n",
      "        #S=sum(l_counts)\n",
      "        #logfile.write('reducer '+word+' ['+','.join([str(c) for c in l_counts])+']='+str(S)+'\\n')\n",
      "        #yield (word, S)\n",
      "\n",
      "if __name__ == '__main__':\n",
      "    MRWeather.run()"
     ],
     "language": "python",
